<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.borneoixora.carparkattendant"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />

    <application
        android:name="com.borneoixora.carparkattendant.MyApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.borneoixora.carparkattendant.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.borneoixora.carparkattendant.ChargeActivity"
            android:label="@string/title_activity_charge" >
        </activity>
        <activity
            android:name="com.borneoixora.carparkattendant.TestPrintActivity"
            android:label="@string/title_activity_test_print" >
        </activity>
        <activity
            android:name="com.borneoixora.carparkattendant.TestUhfActivity"
            android:label="@string/title_activity_test_uhf" >
        </activity>
        <activity
            android:name="com.borneoixora.carparkattendant.PrintActivity"
            android:label="@string/title_activity_print" >
        </activity>
    </application>

</manifest>
